      <div class="container text-center myslider">
        <div class="disslider" style="width:90%; margin: auto;  box-shadow: 0px 5px 10px 10px  rgba(17, 17, 17, 0.5);">
            <div id="carouselExampleAutoplaying" class="carousel slide" data-bs-ride="carousel">
                <div class="carousel-inner">
                    <div class="carousel-item active">
                        <img src="../images/anasayfabg.jpg" class="d-block w-100" style="width: 100%; height: 500px; object-fit: cover;" alt="...">
                    </div>
                    <div class="carousel-item">
                        <img src="../images/datcada-gezilecek-yerler-eski-datca.jpg" class="d-block w-100" style="width: 100%; height: 500px; object-fit: cover;" alt="...">
                    </div>
                    <div class="carousel-item">
                        <img src="../images/ilkbaharda-gezilecek-yerler-listesi-1140x641.jpg" class="d-block w-100" style="width: 100%; height: 500px; object-fit: cover;" alt="...">
                    </div>

                  
                      <script>

    $(document).ready(function() {
        var carouselInner = $(".carousel-inner");

      
        for (var i = 2; i <= 20; i++) {
      
            var carouselItem = $("<div>").addClass("carousel-item");
          
            var imgSrc = "../images/galeri/" + i + ".jpg";
            var imgElement = $("<img>").attr("src", imgSrc).addClass("d-block w-100").css({
                "width": "100%",
                "height": "500px",
                "object-fit": "cover"
            }).attr("alt", "...");
          
            carouselItem.append(imgElement);
          
            carouselInner.append(carouselItem);
        }
    });
</script>
                    </script>
                </div>
                <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleAutoplaying" data-bs-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Previous</span>
                </button>
                <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleAutoplaying" data-bs-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Next</span>
                </button>
            </div>
        </div>
    </div>
    
    
     
      
        
